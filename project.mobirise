verlayColor}}\"></div>\n    <div class mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"row {{contentAlign}}\">\n            <div class=\"col-lg-6 col-md-6 col-12 align-left\">\n                <h2 class=\"mbr-section-subtitle mbr-white mbr-fonts-style mb-3\" data-app-selector=\".mbr-section-subtitle\" mbr-theme-style=\"display-4\" mbr-if=\"showSubitle\">\n                    <b>Welcome to Rudraxis Store</b></h2>\n                <h1 class=\"mbr-section-title mbr-white mbr-fonts-style mb-4\" data-app-selector=\".mbr-section-title\" mbr-theme-style=\"display-5\" mbr-if=\"showTitle\"><b>Tibetan &amp; Nepali Goods</b></h1>\n            </div>\n            <div class=\"col-lg-6 col-md-6 col-12 align-right m-auto\">\n\n                <div class=\"button\" mbr-if=\"showText\">\n                    <p class=\"mbr-text mbr-semibold mbr-fonts-style\" mbr-theme-style=\"display-4\" mbr-if=\"showText\" data-app-selector=\".mbr-text, .mbr-section-btn\"><a href=\"page1.html\" class=\"text-primary\"><b>store</b></a>&nbsp; |&nbsp; <a href=\"page1.html\" class=\"text-primary\"><b>services</b></a></p>\n\n                </div>\n            </div>\n\n        </div>\n    \n<!-- ECWID CATEGORY CODE -->\n<div id=\"my-categories-21305103\"></div>\n<!-- ECWID STORE CODE --> <p></p>\n<div id=\"my-store-21305103\"></div>\n\n        \n        \n    </div>\n</section>",

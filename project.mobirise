          "_customHTML": "<section class=\"maps4 hotelm4_maps4\" group=\"Maps\" data-bg-video=\"{{bg.type == 'video' && bg.value.url}}\" mbr-class=\"{'mbr-parallax-background': bg.parallax}\">\n\n\t<mbr-parameters>\n\t\t<!-- Block parameters controls (Blue \"Gear\" panel) -->\n\t\t<header>Size</header>\n\t\t<input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"8\" value=\"1\">\n\t\t<input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"8\" value=\"1\">\n\t\t<select title=\"Info Columns\" name=\"infoColumns\" condition=\"showInfo\">\n\t\t\t<option value=\"1\">1</option>\n\t\t\t<option value=\"2\" selected>2</option>\n\t\t</select>\n\t\t<header>Show / Hide</header>\n\t\t<input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked>\n\t\t<input type=\"checkbox\" title=\"Subtitle\" name=\"showSubtitle\">\n\t\t<input type=\"checkbox\" title=\"Underline\" name=\"showUnderline\" checked condition=\"showSubtitle\">\n\n\t\t<input type=\"checkbox\" title=\"Form\" name=\"showForm\" checked>\n\t\t<input type=\"checkbox\" title=\"Form Title\" name=\"showFormTitle\" condition=\"showForm\">\n\t\t<input type=\"checkbox\" title=\"Form Subtitle\" name=\"showFormSubtitle\" condition=\"showForm\">\n\t\t<input type=\"checkbox\" title=\"Form Text\" name=\"showFormText\" checked condition=\"showForm\">\n\t\t<input type=\"map\" title=\"Map\" name=\"googleMap\" place-id=\"Boudhanath 6, Kathmandu, Nepal\" value=\"Boudhanath 6, Kathmandu, Nepal\">\n\t\t<input type=\"checkbox\" title=\"Gray Filter\" name=\"grayFilter\">\n\t\t<input type=\"checkbox\" title=\"Show Info\" name=\"showInfo\" checked>\n\t\t<input type=\"checkbox\" title=\"Reverse Content\" name=\"reverseContent\" condition=\"showForm\">\n\t\t<header condition=\"(showUnderline && showSubtitle) || showForm || showInfo\">Colors</header>\n\t\t<input type=\"color\" title=\"Underline\" name=\"underlineColor\" value=\"#878787\" condition=\"showUnderline && showSubtitle\">\n\t\t<input type=\"color\" title=\"Labels\" name=\"labelsColor\" value=\"#ffffff\" condition=\"showForm\">\n\t\t<input type=\"color\" title=\"Input BG\" name=\"inputBg\" value=\"#efefef\" condition=\"showForm\">\n\t\t<input type=\"color\" title=\"Input Text\" name=\"inputTextColor\" value=\"#171b21\" condition=\"showForm\">\n\t\t<input type=\"color\" title=\"Lines\" name=\"linesColor\" value=\"#2e3137\" condition=\"showInfo\">\n\t\t<header>Background</header>\n\t\t<fieldset type=\"background\" name=\"bg\" parallax>\n\t\t\t<input type=\"image\" title=\"Image\" value=\"@ADDONS_PATH@/hotelm4/components/_images/background11.jpg\" parallax>\n\t\t\t<input type=\"color\" title=\"Color\" value=\"#0d1013\" selected>\n\t\t\t<input type=\"video\" title=\"Video\" value=\"https://www.youtube.com/watch?v=2jqUN6d6ZyM\">\n\t\t</fieldset>\n\t\t<input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" checked condition=\"bg.type !== 'color'\">\n\t\t<input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#000000\" condition=\"overlay && bg.type !== 'color'\">\n\t\t<input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.5\" condition=\"overlay && bg.type !== 'color'\">\n\t\t<!-- End block parameters -->\n\t</mbr-parameters>\n\n\t<div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n\t</div>\n\t<div class=\"container mbr-white\">\n\t\t<h3 class=\"mbr-section-subtitle main-subtitle mbr-fonts-style align-center mbr-lighter\" mbr-class=\"{'pb-3': !showUnderline}\" mbr-theme-style=\"display-4\" mbr-if=\"showSubtitle\" data-app-selector=\".main-subtitle, .underline\">\n\t\t\tHOTEL LUXURY\n\t\t</h3>\n\t\t<div class=\"underline align-center pb-3\" mbr-if=\"showUnderline && showSubtitle\">\n\t\t\t<div class=\"line\"></div>\n\t\t</div>\n\t\t<h2 class=\"mbr-section-title main-title mbr-fonts-style align-center\" mbr-theme-style=\"display-1\" mbr-if=\"showTitle\" data-app-selector=\".main-title\"><b>Contact Rudraxis</b></h2>\n\t\t<div class=\"row main-row justify-content-center pt-5\">\n\t\t\t<div class=\"col-md-10 col-lg-6 form-wrapper\" mbr-if=\"showForm\">\n\t\t\t\t<h3 class=\"mbr-section-subtitle form-subtitle mbr-fonts-style pb-3\" mbr-theme-style=\"display-4\" mbr-if=\"showFormSubtitle\" data-app-selector=\".form-subtitle\">\n\t\t\t\t\tCONTACT FORM\n\t\t\t\t</h3>\n\t\t\t\t<h2 class=\"mbr-section-title form-title mbr-fonts-style\" mbr-class=\"{'pb-3': showFormText,\n                                'pb-4': !showFormText}\" mbr-theme-style=\"display-5\" mbr-if=\"showFormTitle\" data-app-selector=\".form-title\">Contact Rudraxis</h2>\n\t\t\t\t<p class=\"mbr-text form-text mbr-fonts-style m-0 pb-4 mbr-lighter\" mbr-theme-style=\"display-7\" mbr-if=\"showFormText\" data-app-selector=\".form-text\">Feel free to write us a message!<br>We will be happy to answer your questions about product we sell.<br></p>\n\t\t\t\t<div class=\"form1\" mbr-form>\n\t\t\t\t\t<!---Formbuilder Form--->\n\t\t\t\t\t<form action=\"buy@rudraxis.com\" name=\"Mobirise Form\" method=\"POST\" class=\"mbr-form form-with-styler\">\n\t\t\t\t\t\t<div class=\"row\">\n\t\t\t\t\t\t\t<div hidden=\"hidden\" data-form-alert class=\"alert alert-success col-12\">Thanks for contacting\n\t\t\t\t\t\t\t\tus!!</div>\n\t\t\t\t\t\t\t<div hidden=\"hidden\" data-form-alert-danger class=\"alert alert-danger col-12\"></div>\n\t\t\t\t\t\t</div>\n\t\t\t\t\t\t<div class=\"dragArea row\">\n\t\t\t\t\t\t\t<div class=\"col-md-12 form-group \" data-for=\"firstname\">\n\t\t\t\t\t\t\t\t<label mbr-text mbr-theme-style=\"display-7\" for=\"firstname\" class=\"form-control-label mbr-fonts-style\">First Name</label>\n\t\t\t\t\t\t\t\t<input type=\"text\" name=\"firstname\" data-form-field=\"First Name\" mbr-theme-style=\"display-7\" required=\"required\" class=\"form-control\">\n\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t\t<div class=\"col-md-12 form-group \" data-for=\"las